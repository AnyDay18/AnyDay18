def password_verification(password1, password2):  
if password1 == password2:  
return "Passwords match. 
Access granted."  
else:  
return "Passwords do not match. Access denied."
def check_even_odd(number):  
if number % 2 == 0:  
return "The number is even."  
else:  
return "The number is odd." 
def greet_user(name):  
return f"Hello, {name}! Welcome back." # Testing the functions 
password1 = "SecurePassword123" password2 = "SecurePassword123" print(password_verification(password1, password2)) 
number = 7 print(check_even_odd(number)) 
name = "Alice"
print(greet_user(name)) 
