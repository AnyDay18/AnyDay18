 pip install matplotlib 
import matplotlib.pyplot as plt import numpy as np 
%matplotlib inline 
x = np.linspace(0, 2 * np.pi, 100) 
sine = np.sin(x) 
cosine = np.cos(x) 
fig, ax = plt.subplots() 
ax.plot(x, sine, label='sine') ax.plot(x, cosine, label='cosine') ax.legend() 
ax.set_xlabel('Angle [rad]') ax.set_ylabel('Function Values') ax.set_title('Sine and Cosine Functions') 
plt.show() 
langs = ['C', 'C++', 'Java', 'Python', 'Oracle'] 
students = [23, 17, 35, 29, 12] 
